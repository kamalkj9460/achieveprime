<script>
      function waitForElm(selector) {
          return new Promise(resolve => {
              if (document.querySelector(selector)) {
                  return resolve(document.querySelector(selector));
              }

              const observer = new MutationObserver(mutations => {
                  if (document.querySelector(selector)) {
                      resolve(document.querySelector(selector));
                      observer.disconnect();
                  }
              });

              observer.observe(document.body, {
                  childList: true,
                  subtree: true
              });
          });
      }
      waitForElm('.mbc-bundle-widget').then((elm) => {
        console.log('Element is ready');
        var lang_active = $('html').attr('lang');
        if (lang_active == 'de') {
          $('.mbc-bundle__custom-button').text('Größentabelle');
        } else {
          $('.mbc-bundle__custom-button').text('Size Guide');
        }
        $('.mbc-bundle__custom-button').attr('href', '#size-chart-content');
        document.addEventListener("MAKEBECOOL_BUNDLES.showSizeGuide", function () {
            e.preventDefault();
            $('.custom_modal').show();
