  .slicks-arrow {
  	width: 38px;
    height: 38px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    cursor: pointer;
  	background: #f3f3f3;
    z-index: 2;
    transition: .3s;
    justify-content: center;
  }
  
  .slicks-arrow:hover {
