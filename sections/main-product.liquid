  	display: none;
  }
  
  .slicks-arrow::after {
  	display: none;
  }
  
  .slicks-arrow svg{
  	fill: #222;
    color: #222
    width: 20px;
    height: 20px;
  }
  
  .slicks-arrow:hover svg {
  	fill: #e0b286!important;
    color: #e0b286!important
  }
  
  .slicks-prev {
  	position: absolute;
   	top:50%;
    left:50%;
    top: 0;
    opacity: 0;
    transform:translate(-50%, -50%);
  }
  
  .slicks-prev_active {
  	opacity: 1;
  }
  
  .slicks-prev svg {
  	 transform: rotate(-90deg);
  }
  
  .slicks-next svg {
  	 transform: rotate(90deg);
  }
  
  .slicks-next {
  	position: absolute;
   	top:93%;
    left:50%;
    transform:translate(-50%, -50%);
  }
  
