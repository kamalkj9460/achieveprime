          {
            "type": "checkbox",
            "id": "show_social_icons",
            "label": "Show social media links",
            "default": true,
            "info": "Add your social media links in Theme Settings > Social media"
          }
        ]
      },
      {
        "type": "newsletter",
        "limit": 1,
        "name": "Newsletter",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Heading",
            "default": "Newsletter"
          },
          {
            "type": "richtext",
            "id": "text",
