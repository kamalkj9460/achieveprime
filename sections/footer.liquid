  {
    "name": "Footer",
    "class": "section-footer",
    "max_blocks": 4,
    "settings": [
 	  {
        "type": "checkbox",
        "id": "fade_amazon",
        "label": "Show amazon",
        "default": true
      },
  	  {
        "type": "header",
        "content": "Cookie popup settings"
      },
	  {
        "type": "checkbox",
        "id": "show_cookie_popup",
        "label": "Show cookie popup",
        "default": true
      },
	  {
        "type": "text",
        "id": "cookie_popup_header_en",
        "label": "Header text English",
        "default": "Your Choice Regarding Cookies on This Site"
      },
	  {
        "type": "text",
        "id": "cookie_popup_header_de",
        "label": "Header text German",
        "default": "Ihre Wahl bezüglich Cookies auf dieser Site"
      },
      {
        "type": "html",
        "id": "cookie_popup_text_en",
        "label": "Text English",
        "default": "The IAPP uses cookies to give users like yourself the best possible content and experience.<a href='/pages/datenschutz'>Learn more</a>"
      },
	  {
        "type": "html",
        "id": "cookie_popup_text_de",
        "label": "Text German",
        "default": "Die IAPP verwendet Cookies, um Benutzern wie Ihnen die bestmöglichen Inhalte und Erfahrungen zu bieten.<a href='/pages/datenschutz'>Weitere Informationen</a>"
      },
	  {
        "type": "color",
        "id": "general_back",
        "label": "Background color",
        "default": "#e0b286"
      },
	  {
        "type": "color",
        "id": "cookie_popup_text_color",
        "label": "Text color",
        "default": "#fff"
      },
	  {
        "type": "color",
        "id": "cookie_popup_button_color",
        "label": "Button background color",
        "default": "#fff"
      },
	  {
        "type": "color",
        "id": "cookie_popup_button_text_color",
        "label": "Button text color",
        "default": "#e0b286"
      },
	  {
        "type": "text",
        "id": "cookie_popup_button_text_en",
        "label": "Button text",
        "default": "Accept"
      },
	  {
        "type": "text",
        "id": "cookie_popup_button_text_de",
        "label": "Button text",
        "default": "Annehmen"
      },
 	  {
        "type": "text",
        "id": "cookie_popup_button_text_en_2",
        "label": "Button text",
        "default": "Decline"
      },
	  {
        "type": "text",
        "id": "cookie_popup_button_text_de_2",
        "label": "Button text",
        "default": "Ablehnen"
      },
      {
        "type": "link_list",
        "id": "lower_menu",
        "label": "Lower footer menu"
      },
      {
        "type": "header",
        "content": "Payment methods"
      },
      {
        "type": "checkbox",
        "id": "show_payment_icons",
        "label": "Show payment icons",
        "default": true,
        "info": "Payment icons are pulled directly from Shopify based on your [payment settings](https://help.shopify.com/manual/payments)"
      },
      {
        "type": "image_picker",
        "id": "custom_payment_icon",
        "label": "Custom payment icon",
        "info": "40 x 25px recommended"
      },
      {
        "type": "header",
        "content": "Country/Region Selector",
        "info": "To add a country/region, go to your [payment settings.](/admin/settings/payments)"
      },
      {
        "type": "checkbox",
        "id": "show_country_selector",
        "label": "Enable country/region selector",
        "default": true
      },
      {
        "type": "header",
        "content": "Language Selector",
        "info": "To add a language, go to your [language settings.](/admin/settings/languages)"
      },
      {
        "type": "checkbox",
        "id": "show_language_selector",
        "label": "Enable language selector",
        "default": true
      }
    ],
    "blocks": [
      {
        "type": "menu",
        "name": "Links",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Heading",
            "default": "Quick links"
          },
          {
            "type": "link_list",
            "id": "menu",
            "label": "Menu",
            "info": "This menu won't show dropdown items",
            "default": "footer"
          }
        ]
      },
      {
        "type": "text",
        "name": "Text/Social",
